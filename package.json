    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-mocha": "^10.5.0",
    "husky": "^9.1.7",
    "mocha": "^10.8.2",
    "prettier": "^3.4.1",
    "pretty-quick": "^4.0.0",
    "release-it": "^17.10.0",
